  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def detect_license_plate(image_path, show_details=True):\n",
    "    \"\"\"\n",
    "    Hàm nhận dạng biển số xe từ ảnh\n",
    "    \n",
    "    Args:\n",
    "        image_path (str): Đường dẫn đến file ảnh\n",
    "        show_details (bool): Có hiển thị chi tiết quá trình không\n",
    "    \n",
    "    Returns:\n",
    "        tuple: (ảnh kết quả, danh sách biển số phát hiện)\n",
    "    \"\"\"\n",
    "    \n",
    "    # Kiểm tra models đã load chưa\n",
    "    if 'yolo_LP_detect' not in globals() or 'yolo_license_plate' not in globals():\n",
    "        print(\"❌ Models chưa được load! Hãy chạy cell load models trước.\")\n",
    "        return None, None\n",
    "        \n",
    "    if yolo_LP_detect is None or yolo_license_plate is None:\n",
    "        print(\"❌ Models chưa được load! Hãy chạy cell load models trước.\")\n",
    "        return None, None\n",
    "    \n",
    "    if show_details:\n",
    "        print(f\"🔍 Đang xử lý ảnh: {image_path}\")\n",
    "    \n",
    "    # Đọc ảnh\n",
    "    img = cv2.imread(image_path)\n",
    "    if img is None:\n",
    "        print(f\"❌ Không thể đọc ảnh: {image_path}\")\n",
    "        return None, None\n",
    "    \n",
    "    if show_details:\n",
    "        print(\"📸 Đang phát hiện biển số...\")\n",
    "    \n",
    "    try:\n",
    "        # Phát hiện biển số\n",
    "        plates = yolo_LP_detect(img, size=640)\n",
    "        list_plates = plates.pandas().xyxy[0].values.tolist()\n",
    "        list_read_plates = set()\n",
    "    except Exception as e:\n",
    "        print(f\"❌ Lỗi khi phát hiện biển số: {e}\")\n",
    "        return None, None\n",
    "    \n",
    "    if show_details:\n",
    "        print(f\"🎯 Phát hiện {len(list_plates)} biển số\")\n",
    "    \n",
    "    if len(list_plates) == 0:\n",
    "        # Nếu không phát hiện được biển số, thử đọc toàn bộ ảnh\n",
    "        if show_details:\n",
    "            print(\"🔄 Thử nhận dạng trên toàn bộ ảnh...\")\n",
    "        try:\n",
    "            lp = helper.read_plate(yolo_license_plate, img)\n",
    "            if lp != \"unknown\":\n",
    "                list_read_plates.add(lp)\n",
    "        except Exception as e:\n",
    "            print(f\"❌ Lỗi khi đọc biển số: {e}\")\n",
    "    else:\n",
    "        # Xử lý từng biển số phát hiện được\n",
    "        for i, plate in enumerate(list_plates):\n",
    "            if show_details:\n",
    "                print(f\"📝 Đang đọc biển số {i+1}/{len(list_plates)}...\")\n",
    "            \n",
    "            try:\n",
    "                x = int(plate[0])  # xmin\n",
    "                y = int(plate[1])  # ymin\n",
    "                w = int(plate[2] - plate[0])  # width\n",
    "                h = int(plate[3] - plate[1])  # height\n",
    "                \n",
    "                # Cắt ảnh biển số\n",
    "                crop_img = img[y:y+h, x:x+w]\n",
    "                \n",
    "                # Vẽ khung biển số lên ảnh gốc\n",
    "                cv2.rectangle(img, (x, y), (x+w, y+h), color=(0, 0, 255), thickness=3)\n",
    "                \n",
    "                # Thử các cách xoay và tăng cường ảnh khác nhau\n",
    "                flag = 0\n",
    "                for cc in range(0, 2):\n",
    "                    for ct in range(0, 2):\n",
    "                        lp = helper.read_plate(yolo_license_plate, utils_rotate.deskew(crop_img, cc, ct))\n",
    "                        if lp != \"unknown\":\n",
    "                            list_read_plates.add(lp)\n",
    "                            flag = 1\n",
    "                            break\n",
    "                    if flag == 1:\n",
    "                        break\n",
    "            except Exception as e:\n",
    "                print(f\"❌ Lỗi khi xử lý biển số {i+1}: {e}\")\n",
    "                continue\n",
    "    \n",
    "    if show_details:\n",
    "        if list_read_plates:\n",
    "            print(f\"✅ Nhận dạng thành công: {list_read_plates}\")\n",
    "        else:\n",
    "            print(\"❌ Không nhận dạng được biển số nào\")\n",
    "    \n",
    "    return img, list_read_plates\n",
    "\n",
    "print(\"✅ Hàm nhận dạng đã sẵn sàng!\")"
   ]
